		<Scripting Language="VBScript" EntryPoint="" Timeout="5000">
			<Code><![CDATA[
function m
dim contentId, recordGuid,cs
contentId = cp.content.getId( "EditModalTestData" )
recordGuid = cp.request.gettext( "instanceId" )

cs = cp.csnew()
call cs.open( "EditModalTestData", "ccguid=" & cp.db.encodesqltext(recordGuid)  )
if (not cs.ok()) then
     call cs.insert("EditModalTestData")
     call cs.setField("ccguid",recordGuid)
     cs.save()
end if

m = cp.content.GetEditWrapper( "<h2>Edit-Modal-Test Widget</h2><p>settings are in EditModalTestData</p>", contentId, recordGuid )

end function			
