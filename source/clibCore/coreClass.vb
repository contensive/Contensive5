                    CS = app.db_csInsertRecord("Content Watch Lists", 0)
                    If app.db_csOk(CS) Then
                        Call app.db_setCS(CS, "name", ListName)
                    End If
                End If
                Call app.db_csClose(CS)
            End If
            '
            Exit Function
ErrorTrap:
            Call handleLegacyError4(Err.Number, Err.Source, Err.Description, "db_csOpenWatchList", True)
        End Function
