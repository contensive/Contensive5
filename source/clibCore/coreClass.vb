            Dim SQL As String
            '
            Select Case main_GetDataSourceType(DataSourceName)
                Case DataSourceTypeODBCMySQL
                    SQL = "SELECT"
                    SQL &= " " & FieldList
                    SQL &= " FROM " & From
                    If Where <> "" Then
                        SQL &= " WHERE " & Where
