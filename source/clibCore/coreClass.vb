        ''' </summary>
        ''' <value></value>
        ''' <returns></returns>
        ''' <remarks>_cluster created during constructor.</remarks>
        Public ReadOnly Property cluster() As coreClusterClass
            Get
                If (_cluster Is Nothing) Then
                    _cluster = New coreClusterClass(Me)
                End If
                Return _cluster
            End Get
        End Property
        Private _cluster As coreClusterClass
