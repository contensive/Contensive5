            Dim RowFieldArray() As String
            Dim RowCnt As Integer
            Dim RowMax As Integer
            Dim ColumnMax As Integer
            Dim RowPointer As Integer
            Dim ColumnPointer As Integer
            Dim DropDownFieldPointer() As Integer
