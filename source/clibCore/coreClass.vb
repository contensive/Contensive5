        '=============================================================================
        '
        Public Sub main_AddGroupMember(ByVal GroupName As String, Optional ByVal NewMemberID As Integer = SystemMemberID, Optional ByVal DateExpires As Date = Nothing)
            On Error GoTo ErrorTrap ''Dim th as integer : th = profileLogMethodEnter("AddGroupMember")
            '
            'If Not (true) Then Exit Sub
            '
            Dim CS As Integer
            Dim GroupID As Integer
            Dim MethodName As String
            Dim iGroupName As String
            Dim iDateExpires As Date
            '
            MethodName = "main_AddGroupMember"
            '
            iGroupName = EncodeText(GroupName)
            iDateExpires = DateExpires 'encodeMissingDate(DateExpires, Date.MinValue)
            '
            If iGroupName <> "" Then
                GroupID = main_GetGroupID(iGroupName)
                If (GroupID < 1) Then
                    GroupID = group_Add(GroupName, GroupName)
                End If
                If (GroupID < 1) Then
                    Call handleLegacyError14(MethodName, "main_AddGroupMember could not find or add Group [" & GroupName & "]")
                Else
                    CS = app.db_csOpen("Member Rules", "(MemberID=" & app.db_EncodeSQLNumber(NewMemberID) & ")and(GroupID=" & app.db_EncodeSQLNumber(GroupID) & ")", , False)
                    If Not app.db_csOk(CS) Then
                        Call app.db_csClose(CS)
                        CS = app.db_csInsertRecord("Member Rules")
                    End If
                    If Not app.db_csOk(CS) Then
                        Call handleLegacyError14(MethodName, "main_AddGroupMember could not add this member to the Group [" & GroupName & "]")
                    Else
                        Call app.db_setCS(CS, "active", True)
                        Call app.db_setCS(CS, "memberid", NewMemberID)
                        Call app.db_setCS(CS, "groupid", GroupID)
                        If iDateExpires <> Date.MinValue Then
                            Call app.db_setCS(CS, "DateExpires", iDateExpires)
                        Else
                            Call app.db_setCS(CS, "DateExpires", Nothing)
                        End If
                    End If
                    Call app.db_csClose(CS)
                End If
            End If
            '
            Exit Sub
            '
            ' ----- Error Trap
            '
ErrorTrap:
            Call handleLegacyError18(MethodName)
            '
        End Sub
