        '
        '========================================================================
        ' Initilize the Current Page using just the WebClient
        '   main_AllowVisitTracking - if true a cookie will be written and the visit tracked
        '========================================================================
        '
        Private Sub web_init_initVisit(ByVal main_AllowVisitTracking As Boolean)
            Try
                Dim NeedToWriteVisitCookie As Boolean
                Dim TrackGuests As Boolean
                Dim TrackGuestsWithoutCookies As Boolean
                Dim DefaultMemberName As String
                'dim buildversion As String
                Dim hint As String
                Dim AllowOnNewVisitEvent As Boolean
                Dim HeaderusernamePassword As String
                'Dim base64 As Base64Class
                Dim CS As Integer
                Dim CSVisit As Integer
                Dim CSNewVisit As Integer
                Dim CSNewVisitor As Integer
                Dim VisitorChanges As Boolean
                Dim MemberChanges As Boolean
                Dim main_VisitLastTimeFromCookie As Date
                Dim SQL As String
                Dim SlashPosition As Integer
                Dim MemberLinkinEID As String
                Dim MemberLinkLoginID As Integer
                Dim MemberLinkRecognizeID As Integer
                Dim CookieVisitNew As String
                Dim CookieVisit As String
                Dim CookieVisitor As String
                Dim WorkingReferer As String
                Dim MethodName As String
                Dim Copy As String
                Dim AuthUsername As String
                Dim AuthPassword As String
                Dim AuthDomain As String
                Dim main_appNameCookiePrefix As String
                '
                main_appNameCookiePrefix = LCase(main_encodeCookieName(app.config.name))

                ' ----- Visit Defaults
                '
                CSNewVisit = -1
                main_VisitId = 0
                main_VisitPages = 0
                main_VisitLoginAttempts = 0
                main_VisitLastTimeFromCookie = Date.MinValue
                main_VisitStateOK = True
                visit_isAuthenticated = False
                main_VisitExcludeFromAnalytics = False
                '
                ' ----- Visitor Defaults
                '
                'hint = "010"
                CSNewVisitor = -1
                main_VisitorID = 0
                main_VisitorNew = False
                main_VisitCookieSupport = False
                VisitorChanges = False
                '
                ' ----- Member Defaults
                '
                'hint = "020"
                userId = 0
                userName = "Guest"
                userAdded = False
                userIsNew = False
                MemberChanges = False
                userStyleFilename = ""
                userExcludeFromAnalytics = False
                '
                ' ----- Decode Browser User-Agent string to main_VisitName, main_VisitIsBot, main_VisitIsBadBot, etc
                '
                ' moved to only run on first page (new visit) hit
                '    Call main_decodeBrowserUserAgent(main_VisitBrowser)
                '    '
                '    ' ----- Determine if Visit Tracking is allowed
                '    '
                '
                'Call AppendLog("main_InitVisit(), 2420")
                '
                Dim tokenDate As Date
                CookieVisit = web_GetStreamCookie(main_appNameCookiePrefix & main_cookieNameVisit)
                MemberLinkinEID = doc_getText("eid")
                MemberLinkLoginID = 0
                MemberLinkRecognizeID = 0
                If (MemberLinkinEID <> "") Then
                    If EncodeBoolean(app.siteProperty_getBoolean("AllowLinkLogin", True)) Then
                        '
                        ' Link Login
                        '
                        Call security_decodeToken(MemberLinkinEID, app.config.privateKey, MemberLinkLoginID, tokenDate)
                        'MemberLinkLoginID = main_DecodeKeyNumber(MemberLinkinEID)
                    ElseIf EncodeBoolean(app.siteProperty_getBoolean("AllowLinkRecognize", True)) Then
                        '
                        ' Link Recognize
                        '
                        Call security_decodeToken(MemberLinkinEID, app.config.privateKey, MemberLinkRecognizeID, tokenDate)
                        'MemberLinkRecognizeID = main_DecodeKeyNumber(MemberLinkinEID)
                    Else
                        '
                        ' block link login
                        '
                        MemberLinkinEID = ""
                    End If
                End If
                'hint = "200"
                If (main_AllowVisitTracking) Or (CookieVisit <> "") Or (MemberLinkLoginID <> 0) Or (MemberLinkRecognizeID <> 0) Then
                    '
                    ' ----- try cookie main_VisitId
                    '
                    'hint = "210"
                    If (CookieVisit <> "") Then
                        Call security_decodeToken(CookieVisit, app.config.privateKey, main_VisitId, main_VisitLastTimeFromCookie)
                        'main_VisitId = main_DecodeKeyNumber(CookieVisit)
                        If main_VisitId = 0 Then
                            '
                            ' ----- Bad Cookie, clear it so a new one will be written
                            '
                            CookieVisit = ""
                        Else
                            '
                            ' ----- good cookie
                            '
                            'main_VisitLastTimeFromCookie = main_DecodeKeyTime(CookieVisit)
                        End If
                    End If
                    '
                    ' ----- Visit is good, read Visit/Visitor
                    '
                    'hint = "220"
                    If (main_VisitId <> 0) Then
                        SQL = "SELECT" _
                            & " ccVisits.ID AS VisitId" _
                            & ",ccVisits.Name AS VisitName" _
                            & ",ccVisits.MemberID AS VisitMemberID" _
                            & ",ccVisits.VisitAuthenticated AS VisitAuthenticated" _
                            & ",ccVisits.StartTime AS VisitStartTime" _
                            & ",ccVisits.StartDateValue AS VisitStartDateValue" _
                            & ",ccVisits.LastVisitTime AS VisitLastVisitTime" _
                            & ",ccVisits.StopTime AS VisitStopTime" _
                            & ",ccVisits.PageVisits AS VisitPageVisits" _
                            & ",ccVisits.CookieSupport AS VisitCookieSupport" _
                            & ",ccVisits.LoginAttempts AS VisitLoginAttempts" _
                            & ",ccVisits.VisitorNew AS VisitVisitorNew" _
                            & ",ccVisits.HTTP_REFERER AS VisitHTTP_REFERER" _
                            & ",ccVisits.REMOTE_ADDR AS VisitREMOTE_ADDR" _
                            & ",ccVisits.Browser AS VisitBrowser" _
                            & ",ccVisits.MemberNew AS VisitMemberNew"
                        SQL &= "" _
                            & ",ccVisitors.ID AS VisitorID" _
                            & ",ccVisitors.Name AS VisitorName" _
                            & ",ccVisitors.MemberID AS VisitorMemberID" _
                            & ",ccVisitors.OrderID AS VisitorOrderID"
                        If True Then
                            SQL &= ",ccVisitors.ForceBrowserMobile AS VisitorForceBrowserMobile"
                        Else
                            SQL &= ",0 AS VisitorForceBrowserMobile"
                        End If
                        If True Then
                            SQL &= ",ccVisits.bot AS VisitBot"
                            SQL &= ",ccVisits.mobile AS VisitMobile"
                        Else
                            SQL &= ",0 AS VisitBot"
                            SQL &= ",0 AS VisitMobile"
                        End If
                        SQL &= "" _
                            & ",m.ID AS MemberID" _
                            & ",m.Name AS MemberName" _
                            & ",m.admin AS MemberAdmin" _
                            & ",m.developer AS MemberDeveloper" _
                            & ",m.ContentControlID AS MemberContentControlID" _
                            & ",m.AllowBulkEmail AS MemberAllowBulkEmail" _
                            & ",m.AllowToolsPanel AS MemberAllowToolsPanel" _
                            & ",m.AdminMenuModeID AS MemberAdminMenuModeID" _
                            & ",m.AutoLogin AS MemberAutoLogin" _
                            & ",m.username AS MemberUsername" _
                            & ",m.password AS MemberPassword" _
                            & ",m.LanguageID AS MemberLanguageID" _
                            & ",ccLanguages.name AS MemberLanguage"
                        ' ----- consider removing these
                        ' 6/18/2009 notes was removed from base
                        '                & ",m.SendNotes AS MemberSendNotes"

                        SQL &= "" _
                            & ",m.OrganizationID AS MemberOrganizationID" _
                            & ",m.Active AS MemberActive" _
                            & ",m.Visits AS MemberVisits" _
                            & ",m.LastVisit AS MemberLastVisit" _
                            & ",m.Company AS MemberCompany" _
                            & ",m.Email AS MemberEmail"
                        If True Then
                            SQL &= ",m.StyleFilename as MemberStyleFilename"
                        Else
                            SQL &= ",'' as MemberStyleFilename"
                        End If
                        If True Then
                            SQL &= ",m.ExcludeFromAnalytics as MemberExcludeFromAnalytics"
                            SQL &= ",ccvisits.ExcludeFromAnalytics as VisitExcludeFromAnalytics"
                        Else
                            SQL &= ",0 as MemberExcludeFromAnalytics"
                            SQL &= ",0 as VisitExcludeFromAnalytics"
                        End If

                        SQL &= "" _
                            & " FROM ((ccVisits" _
                                & " LEFT JOIN ccVisitors ON ccVisits.VisitorID = ccVisitors.ID)" _
                                & " LEFT JOIN ccMembers as m ON ccVisits.MemberID = m.ID)" _
                                & " LEFT JOIN ccLanguages ON m.LanguageID = ccLanguages.ID" _
                            & " WHERE (((ccVisits.ID)=" & main_VisitId & "))"
                        'hint = "230"
                        '
                        'Call AppendLog("main_InitVisit(), 2430")
                        '
                        CS = app.db_csOpenSql(SQL)
                        If Not app.db_csOk(CS) Then
                            '
                            'Call AppendLog("main_InitVisit(), 2435")
                            '
                            ' Bad visit cookie, kill main_VisitId
                            '
                            main_VisitId = 0
                            main_VisitorID = 0
                        Else
                            '
                            'Call AppendLog("main_InitVisit(), 2440")
                            '
                            '--------------------------------------------------------------------------
                            ' ----- Visit found, read visitor info first
                            '--------------------------------------------------------------------------
                            '
                            'hint = "240"
                            main_VisitorID = app.db_GetCSInteger(CS, "VisitorID")
                            main_VisitorName = app.db_GetCSText(CS, "VisitorName")
                            main_VisitorMemberID = (app.db_GetCSInteger(CS, "VisitorMemberID"))
                            main_VisitorForceBrowserMobile = (app.db_GetCSInteger(CS, "VisitorForceBrowserMobile"))
                            main_VisitorOrderID = (app.db_GetCSInteger(CS, "VisitorOrderID"))
                            '
                            '--------------------------------------------------------------------------
                            ' ----- test visit age
                            '--------------------------------------------------------------------------
                            '
                            'hint = "250"
                            main_VisitLastTime = app.db_GetCSDate(CS, "VisitLastVisitTime")
                            If main_VisitLastTime.ToOADate + 0.041666 < main_PageStartTime.ToOADate Then
                                '--------------------------------------------------------------------------
                                ' ----- kill visit (no activity for over 1 hour)
                                ' changed time to 60 minutes from 30 minutes - multiple client request (Toll Brothers, etc)
                                '--------------------------------------------------------------------------
                                '
                                'hint = "251"
                                Call testPoint("main_InitVisit Last visit was more than an hour old, kill the visit")
                                NeedToWriteVisitCookie = True
                                main_VisitId = 0
                                CookieVisit = ""
                            Else
                                '--------------------------------------------------------------------------
                                ' -----  visit OK, capture visit and Member info
                                '--------------------------------------------------------------------------
                                '
                                'hint = "252"
                                main_VisitId = (app.db_GetCSInteger(CS, "VisitId"))
                                main_VisitName = (app.db_GetCSText(CS, "VisitName"))
                                main_VisitCookieSupport = (app.db_GetCSBoolean(CS, "VisitCookieSupport"))
                                main_VisitPages = (app.db_GetCSInteger(CS, "VisitPageVisits"))
                                main_VisitMemberID = (app.db_GetCSInteger(CS, "VisitMemberID"))
                                visit_isAuthenticated = (app.db_GetCSBoolean(CS, "VisitAuthenticated"))
                                main_VisitStartTime = (app.db_GetCSDate(CS, "VisitStartTime"))
                                main_VisitStartDateValue = (app.db_GetCSInteger(CS, "VisitStartDateValue"))
                                main_VisitReferer = (app.db_GetCSText(CS, "VisitHTTP_REFERER"))
                                main_VisitLoginAttempts = (app.db_GetCSInteger(CS, "VisitLoginAttempts"))
                                main_VisitorNew = (app.db_GetCSBoolean(CS, "VisitVisitorNew"))
                                userIsNew = (app.db_GetCSBoolean(CS, "VisitMemberNew"))
                                web.requestRemoteIP = (app.db_GetCSText(CS, "VisitREMOTE_ADDR"))
                                web.requestBrowser = (app.db_GetCSText(CS, "VisitBrowser"))
                                main_VisitTimeToLastHit = 0
                                If main_VisitStartTime > Date.MinValue Then
                                    main_VisitTimeToLastHit = CInt((main_PageStartTime - main_VisitStartTime).TotalSeconds)
                                End If
                                main_VisitExcludeFromAnalytics = app.db_GetCSBoolean(CS, "VisitExcludeFromAnalytics")
                                If ((Not main_VisitCookieSupport) And (CookieVisit <> "")) Then
                                    main_VisitCookieSupport = True
                                End If
                                main_VisitBrowserIsMobile = app.db_GetCSBoolean(CS, "VisitMobile")
                                main_VisitIsBot = app.db_GetCSBoolean(CS, "VisitBot")
                                '
                                '--------------------------------------------------------------------------
                                ' -----  Member info
                                '--------------------------------------------------------------------------
                                '
                                'hint = "260"
                                userId = app.db_GetCSInteger(CS, "MemberID")
                                user_Active = app.db_GetCSBoolean(CS, "MemberActive")
                                'MemberActive = encodeBoolean(MemberActive)
                                If (userId = 0) Or (Not user_Active) Then
                                    '
                                    'Call AppendLog("main_InitVisit(), 2455")
                                    '
                                    ' ----- problem with Member account, main_Get a new one
                                    '
                                    'hint = "261"
                                    userId = 0
                                Else
                                    '
                                    'Call AppendLog("main_InitVisit(), 2460")
                                    '
                                    'hint = "262"
                                    userName = (app.db_GetCSText(CS, "MemberName"))
                                    userIsDeveloper = (app.db_GetCSBoolean(CS, "MemberDeveloper"))
                                    userIsAdmin = (app.db_GetCSBoolean(CS, "MemberAdmin"))
                                    userContentControlID = (app.db_GetCSInteger(CS, "MemberContentControlID"))
                                    user_allowBulkEmail = (app.db_GetCSBoolean(CS, "MemberAllowBulkEmail"))
                                    userAllowToolsPanel = (app.db_GetCSBoolean(CS, "MemberAllowToolsPanel"))
                                    user_adminMenuModeID = app.db_GetCSInteger(CS, "MemberAdminMenuModeID")
                                    user_autoLogin = (app.db_GetCSBoolean(CS, "MemberAutoLogin"))
                                    ' 6/18/2009 notes was removed from base
                                    '                        MemberSendNotes = (app.csv_GetCSBoolean(CS, "MemberSendNotes"))
                                    userUsername = (app.db_GetCSText(CS, "MemberUsername"))
                                    userPassword = (app.db_GetCSText(CS, "MemberPassword"))
                                    userLanguageId = (app.db_GetCSInteger(CS, "MemberLanguageID"))
                                    userLanguage = (app.db_GetCSText(CS, "MemberLanguage"))
                                    userOrganizationId = (app.db_GetCSInteger(CS, "MemberOrganizationID"))
                                    userStyleFilename = app.db_GetCSText(CS, "MemberStyleFilename")
                                    userExcludeFromAnalytics = (app.db_GetCSBoolean(CS, "MemberExcludeFromAnalytics"))
                                    '
                                    ' ----- consider removing
                                    '
                                    userEmail = (app.db_GetCSText(CS, "MemberEmail"))
                                    user_company = (app.db_GetCSText(CS, "MemberCompany"))
                                    user_visits = (app.db_GetCSInteger(CS, "MemberVisits"))
                                    user_lastVisit = (app.db_GetCSDate(CS, "MemberLastVisit"))
                                End If
                                '
                                '--------------------------------------------------------------------------
                                ' ----- set main_VisitStateOK if Dbase main_VisitLastTime matches main_VisitLastTimeFromCookie
                                '--------------------------------------------------------------------------
                                '
                                'hint = "270"
                                If ((main_VisitLastTimeFromCookie - main_VisitLastTime).TotalSeconds) > 2 Then
                                    main_VisitStateOK = False
                                    testPoint("VisitState is false, main_VisitLastTime <> Database main_VisitLastTime, this page is out of order (back button), set main_VisitStateOK false")
                                End If
                            End If
                        End If
                        Call app.db_csClose(CS)
                    End If
                    '
                    '--------------------------------------------------------------------------
                    ' ----- new visit required
                    '--------------------------------------------------------------------------
                    '
                    'Call AppendLog("main_InitVisit(), 2470")
                    '
                    'hint = "300"
                    ' 1/15/2010
                    If (main_VisitId = 0) Then
                        '
                        ' ----- Decode Browser User-Agent string to main_VisitName, main_VisitIsBot, main_VisitIsBadBot, etc
                        '
                        Call web_init_decodeBrowserUserAgent(web.requestBrowser)
                        '
                        ' ----- create new visit record
                        '
                        'hint = "310"
                        main_VisitId = app.metaData_InsertContentRecordGetID("Visits", userId)
                        If (main_VisitId < 1) Then
                            main_VisitId = 0
                            handleException(New Exception("Internal error, new visit record could not be selected."))
                        End If
                        If main_VisitName = "" Then
                            main_VisitName = "User"
                        End If
                        main_VisitPages = 0
                        main_VisitStartTime = main_PageStartTime
                        main_VisitStartDateValue = CInt(main_PageStartTime.ToOADate)
                        main_VisitLastTime = main_PageStartTime
                        '
                        ' ----- main_Get visit referer
                        '
                        'hint = "320"
                        If web.requestReferrer <> "" Then
                            WorkingReferer = web.requestReferrer
                            SlashPosition = InStr(1, WorkingReferer, "//")
                            If (SlashPosition <> 0) And (Len(WorkingReferer) > (SlashPosition + 2)) Then
                                WorkingReferer = Mid(WorkingReferer, SlashPosition + 2)
                            End If
                            SlashPosition = InStr(1, WorkingReferer, "/")
                            If SlashPosition = 0 Then
                                main_VisitRefererPathPage = ""
                                main_VisitRefererHost = WorkingReferer
                            Else
                                main_VisitRefererPathPage = Mid(WorkingReferer, SlashPosition)
                                main_VisitRefererHost = Mid(WorkingReferer, 1, SlashPosition - 1)
                            End If
                        End If
                        '
                        '--------------------------------------------------------------------------
                        ' ----- create visitor from cookie
                        '--------------------------------------------------------------------------
                        '
                        'hint = "330"
                        CookieVisitor = EncodeText(web_GetStreamCookie(main_appNameCookiePrefix & main_cookieNameVisitor))
                        '
                        'Call AppendLog("main_InitVisit(), 2480")
                        '
                        If EncodeBoolean(app.siteProperty_getBoolean("AllowAutoRecognize", True)) Then
                            '
                            'Call AppendLog("main_InitVisit(), 2485")
                            '
                            'hint = "340"
                            Call security_decodeToken(CookieVisitor, app.config.privateKey, main_VisitorID, tokenDate)
                            'main_VisitorID = main_DecodeKeyNumber(CookieVisitor)
                            If main_VisitorID <> 0 Then
                                '
                                ' ----- cookie found, open visitor
                                '
                                'hint = "350"
                                main_VisitCookieSupport = True
                                If True Then
                                    SQL = "SELECT ID,Name,MemberID,OrderID,ForceBrowserMobile from ccVisitors WHERE ID=" & main_VisitorID & ";"
                                Else
                                    SQL = "SELECT ID,Name,MemberID,OrderID,0 as ForceBrowserMobile from ccVisitors WHERE ID=" & main_VisitorID & ";"
                                End If
                                CS = app.db_csOpenSql(SQL)
                                If Not app.db_csOk(CS) Then
                                    '
                                    ' ----- bad cookie, kill main_VisitorID
                                    '
                                    main_VisitorID = 0
                                Else
                                    '
                                    ' ----- set visitor values
                                    '
                                    VisitorChanges = False
                                    main_VisitorID = (app.db_GetCSInteger(CS, "ID"))
                                    main_VisitorName = (app.db_GetCSText(CS, "Name"))
                                    main_VisitorMemberID = (app.db_GetCSInteger(CS, "MemberID"))
                                    main_VisitorForceBrowserMobile = (app.db_GetCSInteger(CS, "ForceBrowserMobile"))
                                    main_VisitorOrderID = (app.db_GetCSInteger(CS, "OrderID"))
                                End If
                                Call app.db_csClose(CS)
                            End If
                        End If
                        '
                        '--------------------------------------------------------------------------
                        ' ----- create new visitor for new visit
                        '--------------------------------------------------------------------------
                        '
                        'Call AppendLog("main_InitVisit(), 2490")
                        '
                        'hint = "400"
                        If main_VisitorID = 0 Then
                            '
                            ' Visitor Fields
                            '
                            main_VisitorID = app.metaData_InsertContentRecordGetID("Visitors", userId)
                            If (main_VisitorID < 1) Then
                                Call handleLegacyError14(MethodName, "main_InitVisit, could not create new visitor")
                                main_VisitorID = 0
                            End If
                            main_VisitorName = "Visitor " & main_VisitorID
                            main_VisitorMemberID = 0
                            main_VisitorOrderID = 0
                            VisitorChanges = True
                            main_VisitorForceBrowserMobile = 0
                            '
                            ' Visit Fields
                            '
                            main_VisitorNew = True
                        End If
                        '
                        '-----------------------------------------------------------------------------------
                        ' ----- find  identity from the visitor
                        '-----------------------------------------------------------------------------------
                        '
                        'Call AppendLog("main_InitVisit(), 2492")
                        '
                        'hint = "500"
                        main_VisitMemberID = main_VisitorMemberID
                        If (main_VisitorMemberID > 0) Then
                            '
                            ' ----- recognize by the main_VisitorMemberID
                            '
                            'hint = "510"
                            If useer_RecognizeMemberByID(main_VisitorMemberID) Then
                                '
                                ' ----- if successful, now test for autologin (authentication)
                                '
                                'hint = "520"

                                If (EncodeBoolean(app.siteProperty_getBoolean("AllowAutoLogin", False))) And (user_autoLogin) And main_VisitCookieSupport Then
                                    '
                                    ' ----- they allow it, now Check if they were logged in on their last visit
                                    '
                                    'hint = "530"
                                    SQL = "select top 1 V.VisitAuthenticated from ccVisits V where (V.ID<>" & main_VisitId & ")and(V.VisitorID=" & main_VisitorID & ") order by id desc"
                                    CS = app.db_csOpenSql(SQL)
                                    If app.db_csOk(CS) Then
                                        If main_GetCSBoolean(CS, "VisitAuthenticated") Then
                                            '
                                            ' ----- yes, go ahead with autologin
                                            '
                                            If user_LoginMemberByID(userId) Then
                                                Call main_LogActivity2("autologin", userId, userOrganizationId)
                                                VisitorChanges = True
                                                MemberChanges = True
                                            End If
                                        End If
                                    End If
                                    Call app.db_csClose(CS)
                                Else
                                    '
                                    ' Recognized, not auto login
                                    '
                                    'hint = "540"
                                    Call main_LogActivity2("recognized", userId, userOrganizationId)
                                End If
                            End If
                        End If
                        '
                        '--------------------------------------------------------------------------
                        ' ----- new visit, update the persistant visitor cookie
                        '--------------------------------------------------------------------------
                        '
                        'hint = "600"
                        If main_AllowVisitTracking Then
                            Call inet_addResponseCookie(main_appNameCookiePrefix & main_cookieNameVisitor, security_encodeToken(main_VisitorID, main_VisitStartTime), main_VisitStartTime.AddYears(1), , app.RootWebPath, False)
                        End If
                        '
                        '--------------------------------------------------------------------------
                        ' ----- OnNewVisit Add-on call
                        '--------------------------------------------------------------------------
                        '
                        AllowOnNewVisitEvent = True
                    End If
                    '
                    '-----------------------------------------------------------------------------------
                    ' ----- Attempt Link-in recognize or login
                    ' ----- This is allowed even if main_AllowVisitTracking is off
                    '-----------------------------------------------------------------------------------
                    '
                    'Call AppendLog("main_InitVisit(), 2494")
                    '
                    'hint = "700"
                    If (MemberLinkLoginID <> 0) Then
                        '
                        ' Link Login
                        '
                        If user_LoginMemberByID(MemberLinkLoginID) Then
                            Call main_LogActivity2("link login with eid " & MemberLinkinEID, userId, userOrganizationId)
                        End If
                    ElseIf (MemberLinkRecognizeID <> 0) Then
                        '
                        ' Link Recognize
                        '
                        Call useer_RecognizeMemberByID(MemberLinkRecognizeID)
                        Call main_LogActivity2("link recognize with eid " & MemberLinkinEID, userId, userOrganizationId)
                    End If
                    '
                    '-----------------------------------------------------------------------------------
                    ' ----- create guest identity if no identity
                    '-----------------------------------------------------------------------------------
                    '
                    'Call AppendLog("main_InitVisit(), 2496")
                    '
                    'hint = "800"
                    If (userId < 1) Then
                        '
                        ' No user created
                        '
                        If (LCase(Left(main_VisitName, 5)) <> "visit") Then
                            DefaultMemberName = main_VisitName
                        Else
                            DefaultMemberName = EncodeText(db_GetContentFieldProperty("people", "name", "default"))
                        End If
                        If (False) Then
                            '
                            ' not upgraded, just create user like it did before
                            '
                            Call user_CreateUser()
                        Else
                            '
                            ' upgraded, determine the kind of tracking - experimental build set to true
                            '
                            TrackGuests = EncodeBoolean(app.siteProperty_getBoolean("track guests", False))
                            If Not TrackGuests Then
                                '
                                ' do not track guests at all
                                '
                                Call user_CreateUserDefaults(DefaultMemberName)
                            Else
                                If main_VisitCookieSupport Then
                                    '
                                    ' cookies supported, not first hit and not spider
                                    '
                                    Call user_CreateUser()
                                Else
                                    '
                                    ' upgraded, set it to the site property - experimental build set to true
                                    '
                                    TrackGuestsWithoutCookies = EncodeBoolean(app.siteProperty_getBoolean("track guests without cookies", False))
                                    If TrackGuestsWithoutCookies Then
                                        '
                                        ' compatibiltiy mode - create people for non-cookies too
                                        '
                                        Call user_CreateUser()
                                    Else
                                        '
                                        ' set defaults for people record
                                        '
                                        Call user_CreateUserDefaults(DefaultMemberName)
                                    End If
                                End If
                            End If
                        End If
                    End If
                    '
                    '--------------------------------------------------------------------------
                    ' ----- establish language for the member, if they do not have one
                    '--------------------------------------------------------------------------
                    '
                    'Call AppendLog("main_InitVisit(), 2498")
                    '
                    'hint = "900"
                    If main_VisitPages = 0 Then
                        '
                        ' First page of this visit, verify the member language
                        '
                        If (userLanguageId < 1) Or (userLanguage = "") Then
                            '
                            ' No member language, set member language from browser language
                            '
                            Call web_GetBrowserLanguage(userLanguageId, userLanguage)
                            If userLanguageId > 0 Then
                                '
                                ' Browser Language worked
                                '
                                MemberChanges = True
                            Else
                                '
                                ' Still no match, main_Get the default language
                                '
                                userLanguage = app.siteProperty_getText("Language", "English")
                                If userLanguage <> "English" Then
                                    '
                                    ' Handle the non-English case first, so if there is a problem, fall back is English
                                    '
                                    CS = app.db_csOpen("Languages", "name=" & app.db_EncodeSQLText(userLanguage))
                                    If app.db_csOk(CS) Then
                                        userLanguageId = app.db_GetCSInteger(CS, "ID")
                                        MemberChanges = True
                                    End If
                                    Call app.db_csClose(CS)
                                    If userLanguageId = 0 Then
                                        '
                                        ' non-English Language is not in Language Table, set default to english
                                        '
                                        userLanguage = "English"
                                        Call app.siteProperty_set("Language", userLanguage)
                                    End If
                                End If
                                If userLanguage = "English" Then
                                    CS = app.db_csOpen("Languages", "name=" & app.db_EncodeSQLText(userLanguage))
                                    If app.db_csOk(CS) Then
                                        userLanguageId = app.db_GetCSInteger(CS, "ID")
                                        MemberChanges = True
                                    End If
                                    Call app.db_csClose(CS)
                                    If userLanguageId < 1 Then
                                        '
                                        ' English is not in Language table, add it, and set it in Member
                                        '
                                        CS = app.db_csInsertRecord("Languages")
                                        If app.db_csOk(CS) Then
                                            userLanguageId = app.db_GetCSInteger(CS, "ID")
                                            userLanguage = "English"
                                            Call app.db_setCS(CS, "Name", userLanguage)
                                            Call app.db_setCS(CS, "HTTP_Accept_LANGUAGE", "en")
                                            MemberChanges = True
                                        End If
                                        Call app.db_csClose(CS)
                                    End If
                                End If
                            End If
                        End If
                    End If
                    '
                    '-----------------------------------------------------------------------------------
                    ' ----- Save anything that changed
                    '-----------------------------------------------------------------------------------
                    '
                    'Call AppendLog("main_InitVisit(), 2499")
                    '
                    ' can not count main_VisitCookieSupport yet, since a new visit will not show cookie support until the ajax hit
                    'hint = "910"
                    main_VisitExcludeFromAnalytics = main_VisitExcludeFromAnalytics Or main_VisitIsBot Or userExcludeFromAnalytics Or userIsAdmin Or userIsDeveloper
                    '
                    ' Update Page count
                    '
                    If Not web_PageExcludeFromAnalytics Then
                        main_VisitPages = main_VisitPages + 1
                    End If
                    '
                    ' Update the Visit
                    ' set main_visitInitialized true allows main_SaveVisit, main_SaveVisitor, etc to work
                    '
                    visit_initialized = True
                    Call main_SaveVisit()
                    '
                    ' ----- Save visitor record
                    '
                    'hint = "940"
                    If VisitorChanges Then
                        Call main_SaveVisitor()
                    End If
                    '
                    ' ----- Save Member record
                    '
                    'hint = "950"
                    If MemberChanges Then
                        Call user_SaveMemberBase()
                    End If
                    '
                    ' ----- send visit cookie if supported or first page
                    '       no, always send the cookie. There are too many exceptions to try being tricky here.
                    '
                    'hint = "960"
                    CookieVisitNew = security_encodeToken(main_VisitId, main_VisitLastTime)
                    'CookieVisitNew = encodeToken(main_VisitId, main_VisitStartTime)
                    If main_AllowVisitTracking And (CookieVisit <> CookieVisitNew) Then
                        CookieVisit = CookieVisitNew
                        NeedToWriteVisitCookie = True
                    End If
                End If
                ' set visitinitialized - for the cases where the earlier set was bypassed, like now allowvisittracking
                visit_initialized = True
                'hint = "970"
                If (AllowOnNewVisitEvent) And (True) Then
                    '
                    '---------------------------------------------------------------------------------
                    ' ----- OnNewVisitEvent
                    '---------------------------------------------------------------------------------
                    '
                    'hint = "980"
                    '$$$$$ cache this
                    ' $$$$$ make ptr list on load
                    CS = app.db_csOpen("Add-ons", "(OnNewVisitEvent<>0)", "Name", , , , , "id")
                    Do While app.db_csOk(CS)
                        Call main_GetAddonContent(app.db_GetCSInteger(CS, "ID"), "", "", addonContextEnum.ContextOnNewVisit, "", 0, "", 0)
                        app.db_csGoNext(CS)
                    Loop
                    Call app.db_csClose(CS)
                End If
                '
                '---------------------------------------------------------------------------------
                ' ----- Write Visit Cookie
                '---------------------------------------------------------------------------------
                '
                CookieVisit = security_encodeToken(main_VisitId, main_PageStartTime)
                Call inet_addResponseCookie(main_appNameCookiePrefix & main_cookieNameVisit, CookieVisit, , , app.RootWebPath, False)
            Catch ex As Exception
                handleException(ex)
            End Try
        End Sub
