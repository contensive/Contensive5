                        & kmaIndent(PageCopy) _
                        & cr3 & "</p>" _
                        & cr2 & "</td></tr></table>" _
                        & cr & "</div>" _
                        & kmaIndent(main_GetEndOfBody(False, False, False, False)) _
                        & cr & "</body>" _
                        & vbCrLf & "</html>" _
