                Key = Replace(Key, " ", "_")
            End If
            If web.requestDotAsUnderscore Then
                Key = Replace(Key, ".", "_")
            End If
            If (Key <> "") And (web_InStreamArrayCount > 0) Then
                For Pointer = 0 To web_InStreamArrayCount - 1
                    If Key = UCase(web_InStreamArray(Pointer).Name) Then
                        main_InStream = True
                        Exit For
                    End If
                Next
            End If
            '
            Exit Function
