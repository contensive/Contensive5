                    End If
                    If OrderBy <> "" Then
                        SQL &= " ORDER BY " & OrderBy
                    End If
                    If GroupBy <> "" Then
                        SQL &= " GROUP BY " & GroupBy
                    End If
            End Select
            main_GetSQLSelect = SQL
