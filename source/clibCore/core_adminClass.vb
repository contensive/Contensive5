        '==============================================================================================
        '   If this field has no help message, check the field with the same name from it's inherited parent
        '==============================================================================================
        '
        Private Sub getFieldHelpMsgs(ContentID As Integer, FieldName As String, ByRef return_Default As String, ByRef return_Custom As String)
            On Error GoTo ErrorTrap ''Dim th as integer : th = profileLogAdminMethodEnter( "getFieldHelpMsgs")
            '
            Dim SQL As String
            Dim CS As Integer
            Dim Found As Boolean
            Dim ParentID As Integer
            '
            Found = False
            SQL = "select h.HelpDefault,h.HelpCustom from ccfieldhelp h left join ccfields f on f.id=h.fieldid where f.contentid=" & ContentID & " and f.name=" & EncodeSQLText(FieldName)
            CS = cpCore.app.db_csOpenSql(SQL)
            If cpCore.app.db_csOk(CS) Then
                Found = True
                return_Default = cpCore.main_GetCSText(CS, "helpDefault")
                return_Custom = cpCore.main_GetCSText(CS, "helpCustom")
            End If
            Call cpCore.app.db_csClose(CS)
            '
            If Not Found Then
                ParentID = 0
                SQL = "select parentid from cccontent where id=" & ContentID
                CS = cpCore.app.db_csOpenSql(SQL)
                If cpCore.app.db_csOk(CS) Then
                    ParentID = cpCore.app.db_GetCSInteger(CS, "parentid")
                End If
                Call cpCore.app.db_csClose(CS)
                If ParentID <> 0 Then
                    Call getFieldHelpMsgs(ParentID, FieldName, return_Default, return_Custom)
                End If
            End If
            '
            Exit Sub
            '
ErrorTrap:
            Call cpCore.handleException(New Exception("unexpected exception"))
        End Sub
