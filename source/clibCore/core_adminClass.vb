                QS = cpCore.main_RefreshQueryString
                If MenuDepth = 0 Then
                    RightSide = RightSide & "<img alt=""space"" src=""/ccLib/images/spacer.gif"" width=""1"" height=""16"" >"
                    If AdminMenuModeID = AdminMenuModeTop Then
                        QS = ModifyQueryString(QS, "mm", "1", True)
