                            For Ptr = 0 To .GroupListCnt - 1
                                If VarText = .GroupList(Ptr) Then
                                    Exit For
                                End If
                            Next
                        End If
                        If Ptr = .GroupListCnt Then
                            ReDim Preserve .GroupList(.GroupListCnt)
                            .GroupList(.GroupListCnt) = VarText
